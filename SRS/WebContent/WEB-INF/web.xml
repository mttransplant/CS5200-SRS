<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="true" version="3.0">
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Admin</servlet-name>
    <servlet-class>servlet.Admin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddStudent</servlet-name>
    <servlet-class>servlet.AddStudent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddStudent</servlet-name>
    <url-pattern>/addNewStudent</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>NewStudent</servlet-name>
    <servlet-class>servlet.NewStudent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewStudent</servlet-name>
    <url-pattern>/addStudent</url-pattern>
  </servlet-mapping>
   <servlet>
   <servlet-name>ViewStudentInfo</servlet-name>
   <servlet-class>servlet.ViewStudentInfo</servlet-class>
 </servlet>
 <servlet-mapping>
   <servlet-name>ViewStudentInfo</servlet-name>
   <url-pattern>/viewStudent</url-pattern>
 </servlet-mapping>
</web-app>